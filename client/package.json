    "@material-ui/core": "^3.9.3",
    "@material-ui/styles": "^3.0.0-alpha.10",
    "axios": "^0.18.1",
    "bootstrap": "^4.3.1",
    "chart.js": "^2.8.0",
    "classnames": "^2.2.6",
    "core-js": "^2.6.5",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint-config-airbnb": "^17.1.0",
    "flag-icon-css": "^3.3.0",
    "font-awesome": "^4.7.0",
    "formik": "^1.5.4",
    "jwt-decode": "^2.2.0",
    "node-sass": "^4.11.0",
    "prop-types": "^15.7.2",
    "query-string": "^5.1.1",
    "react": "^16.8.5",
    "react-app-polyfill": "^0.2.2",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.8.5",
    "react-jss": "^8.6.1",
    "react-parallax": "^2.2.0",
    "react-redux": "^7.0.2",
    "react-router-config": "^4.4.0-beta.8",
    "react-router-dom": "~4.3.1",
    "react-test-renderer": "^16.8.5",
    "reactstrap": "^7.1.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "simple-line-icons": "^2.4.1",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^2.1.0",
    "lint-staged": "^8.1.5",
    "prettier": "1.17.0",
    "react-scripts": "3.0.0"
  },
  "bugs": {
    "url": "https://bitbucket.org/robthedev/librarytrackr/issues?status=new&status=open"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "proxy": "process.env.port",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/*index.js",
      "!src/serviceWorker.js",
      "!src/polyfill.js"
    ]
  }
